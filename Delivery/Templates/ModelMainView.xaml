<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime"
                    xmlns:viewModels="clr-namespace:Delivery.WPF.ViewModels"
                    xmlns:view ="clr-namespace:Delivery.WPF.Views"
                    >
     

    <DataTemplate DataType="{x:Type viewModels:ClientsViewModel}">
        <view:ClientsView DataContext="{Binding}"/>
    </DataTemplate>
   
    <DataTemplate DataType="{x:Type viewModels:CouriersViewModel}">
        <view:CouriersView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:OrdersViewModel}">
        <view:OrdersView DataContext="{Binding}"/>
    </DataTemplate>



</ResourceDictionary>